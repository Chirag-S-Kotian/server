{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/users/(.*)",
        "dest": "/src/routes/userRoutes.ts"
      },
      {
        "src": "/api/files/(.*)",
        "dest": "/src/routes/fileRoutes.ts"
      }
    ],
    "build": {
      "env": {
        "NODE_ENV": "production"
      }
    }
  }
  